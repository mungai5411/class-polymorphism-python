class Vehicle:
    def move(self):
        pass  # Abstract method


class Car(Vehicle):
    def move(self):
        return "Driving"


class Plane(Vehicle):
    def move(self):

class Boat(Vehicle):
    def move(self):
        return "Sailing"

car = Car()
plane = Plane()
boat = Boat()

vehicles = [car, plane, boat]
for vehicle in vehicles:
    print(vehicle.move())

